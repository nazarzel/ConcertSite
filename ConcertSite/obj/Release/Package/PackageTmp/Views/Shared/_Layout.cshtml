<!DOCTYPE html>
<html>
<head>
    <title></title>
    <meta charset="utf-8">
    <link rel="stylesheet" href="~/css/reset.css" type="text/css" media="screen">
    <link rel="stylesheet" href="~/css/style.css" type="text/css" media="screen">
    <link rel="stylesheet" href="~/css/grid.css" type="text/css" media="screen">

    <script src="js/jquery-1.6.3.min.js" type="text/javascript"></script>
    <script src="~/js/cufon-yui.js" type="text/javascript"></script>
    <script src="~/js/cufon-replace.js" type="text/javascript"></script>
    <script src="~/js/Vegur_700.font.js" type="text/javascript"></script>
    <script src="~/js/Vegur_400.font.js" type="text/javascript"></script>
    <script src="~/js/FF-cash.js" type="text/javascript"></script>
    <script src="~/js/script.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/js/easyTooltip.js"></script>
</head>
<body id="page1">
    <div class="extra">
        <!--==============================header=================================-->
        <header>
            <div class="main">
                <div class="bg-1">
                    <h1><a href="index.html">Rock Bandssss</a></h1>
                </div>
                @if (Request.IsAuthenticated)
                {
                    <h4 id="log">Hello, <span id="log1">@User.Identity.Name</span> @Html.ActionLink("Log out", "LogOff", "Account")</h4>
                }
                else
                {
                    
                    <h4 id="log">@Html.ActionLink("Log in", "Login", "Account")</h4>
                }
                <nav>
                    <div class="menu-bg-tail">
                        <div class="menu-bg">
                            <div class="container_12">
                                <div class="grid_12">
                                    <ul class="menu">
                                        <li class="item">@Html.ActionLink("Main", "Index", "Home")</li>
                                        <li>@Html.ActionLink("Gallery", "Gallery", "Home")</li>
                                        <li>@Html.ActionLink("Company", "Company", "Account")</li>
                                        <li>@Html.ActionLink("Messages", "Messages", "Account")</li>
                                        <li class="item-1">@Html.ActionLink("Live", "Tours", "Home")</li>
                                        <li class="last">@Html.ActionLink("Cabinet", "Cabinet", "Account")</li>
                                    </ul>
                                    <div class="clear"></div>
                                </div>
                                <div class="clear"></div>
                            </div>
                        </div>
                    </div>
                </nav>
                @RenderSection("slider", required: false)
            </div>

        </header>
        <!--==============================content================================-->
        <section id="content">
            <div class="ic">More Website Templates TemplateMonster.com. November 21, 2011!</div>
            @RenderBody()

        </section>
    </div>
    <!--==============================footer=================================-->
    <footer>
        <div class="main">
            <div class="footer-bg">
                <div class="container_12">
                    <div class="wrapper">
                        <div class="grid_12">
                            <div class="footer-padding">
                                <div class="wrapper">
                                    <span class="footer-link"><span>nazar.svyryd@gmail.com &copy; 2016</span> Website done by <a rel="nofollow" target="_blank" class="link" href="http://vk.com/nazarzel">Nazar Svyryd</a></span>
                                    <ul class="list-services">
                                        <li><a class="tooltips n-1" target="_blank" title="Twitter" href="https://twitter.com/rv_Nazar"></a></li>
                                        <li><a class="tooltips n-2" target="_blank" title="Facebook" href="https://www.facebook.com/nazar.svyryd"></a></li>
                                        <li class="last"><a class="tooltips n-3" target="_blank" title="Youtube" href="https://www.youtube.com/channel/UCTcexS6A_xP82vdgpzsIWnA"></a></li>
                                    </ul>
                                </div>
                                <div class="aligncenter">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <script type="text/javascript"> Cufon.now(); </script>
</body>
</html>
